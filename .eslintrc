{
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "strict": [2, "never"],
      "semi": [2, "never"],
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/react-in-jsx-scope": 2,
      "prefer-template": 2,
      "react/jsx-filename-extension": 2,
      "react/jsx-no-target-blank": 2,
      "no-underscore-dangle": 0,
      "no-console": 1
    },
    "plugins": [
      "react"
    ]
  }
  